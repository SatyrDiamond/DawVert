
[socalabs_compressor]
>input
	type|universal:compressor
	param|attack
	param|release
	param|ratio
	param|threshold
	param|knee
	param|pregain
	param|postgain
<input

>proc
	calc|attack|mul|1000
	calc|release|mul|1000
<proc

>output
	type|socalabs:compressor
	param|attack
	param|release
	param|ratio
	param|threshold
	param|knee
	param|input<pregain
	param|output<postgain
<output

