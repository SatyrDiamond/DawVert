<plugstatetransform daw_out="flp" plugtype="universal:volpan">
  <in__param from="pan" type="float" />
  <in__param from="vol" type="float" />

  <calc name="pan" type="mul">128</calc>
  <calc name="vol" type="mul">256</calc>
  
  <replace plugtype="native:flstudio:fruity balance" />

  <out__param to="pan" type="float" />
  <out__param to="vol" type="float" />
</plugstatetransform>