  <in__param from="ceiling" type="float" />
  <in__param from="gain" type="float" />
  <in__param from="release" type="float" />
  
  <calc name="release" type="mul">1000</calc>

  <replace plugtype="native:tracktion:limiter" />

  <out__param to="ceiling" type="float" />
  <out__param to="gain" type="float" />
  <out__param to="release" type="float" />
</plugstatetransform>