<plugstatetransform daw_in="tracktion_edit" plugtype="native:tracktion:limiter">
  <in__param from="ceiling" type="float" />
  <in__param from="gain" type="float" />
  <in__param from="release" type="float" />
  
  <calc name="release" type="div">1000</calc>

  <replace plugtype="universal:limiter" />

  <out__param to="ceiling" type="float" />
  <out__param to="pregain" from="gain" type="float" />
  <out__param to="release" type="float" />
</plugstatetransform>