  <in__param to="1_freq" type="float" />
  <in__param to="1_gain" type="float" />
  <in__param to="1_q" type="float" />
  <in__param to="1_type" type="float" />
  <calc name="1_q" type="sub">0.5</calc>
  <calc name="1_q" type="pow_r">10000</calc>
  <calc name="1_gain" type="from_one">-24;24</calc>
  <calc name="1_type" type="mul">6</calc>
  <toint name="1_type"/>
  <remap name="1_type" remapdef="filter_types"/>

