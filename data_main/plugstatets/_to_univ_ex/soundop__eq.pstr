    <val in_val="1" out_val="low_shelf"/>
    <val in_val="2" out_val="high_shelf"/>
    <val in_val="3" out_val="high_pass"/>
    <val in_val="4" out_val="low_pass"/>
    <val in_val="5" out_val="band_pass"/>
